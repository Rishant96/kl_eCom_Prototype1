@{
    ViewBag.Title = "Home Page";
}
<div style="margin: 0 -15px;">
    <div class="jumbotron row" style="margin: 0;">
        <h1 class="col-md-offset-1 col-md-3" style="position: relative; bottom: 5px; color: darkslategray; padding-left: 50px">Khushlife<br />E-Com</h1>
        <div class="col-md-8 row" style="margin-top: 20px;">
            <div class="col-md-offset-2 col-md-4">
                <img src="~/assets/images/shopping.png" style="display: block; min-width: 200px;
                    min-height: 120px; max-width: 200px; max-height: 120px;
                    border-radius: 10px" />
                <div style="width: 200px; text-align:center; margin-top: 10px">
                    @Html.ActionLink("Shop", "Index", controllerName: "Market")
                </div>
            </div>
            <div class="col-md-offset-1 col-md-4 col-md-offset-1">
                <img src="~/assets/images/open-business.jpg" style="display: block; min-width: 200px;
                    min-height: 120px; max-width: 200px; max-height: 120px;
                    border-radius: 10px" />
                <div style="width: 200px; text-align:center; margin-top: 10px">
                    @Html.ActionLink("Vendors Section", "Index", "Home", new { area = "Vendors" }, null)
                </div>
            </div>
        </div>
    </div>
    <img src="~/assets/images/1-1355x565.jpg" style="width: 100%">
</div>
