@model kl_eCom.Web.Models.MarketIndexViewModel
@{
    Layout = "~/Views/Shared/_EcomLayout.cshtml";
    ViewBag.Title = "Index";
}

<h2>Vendors List</h2>

@foreach (var vendor in Model.Vendors.Keys)
{
    <div style="margin-left: 25px">@Html.ActionLink(Model.Vendors[vendor], "Shops", new { vendorId = vendor })</div>
    <br />
}